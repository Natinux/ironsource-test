{
  "name": "ironsource-test",
  "version": "1.0.0",
  "description": "test for ironSource",
  "main": "app.js",
  "scripts": {
    "build": "node node_modules/gulp/bin/gulp.js build",
    "build-docker": "docker build -t ironsource/test -f ./assets/docker/dockerfile.yml .",
    "start": "cd ./dist/server && node app.js",
    "start-docker": "docker-compose -f ./assets/docker/compose/run.yml up",
    "stop-docker": "docker-compose -f ./assets/docker/compose/run.yml down",
    "test": "node dist/server/test/mocha-runner.js dist/server/test/**/*.spec.js",
    "test-docker": "docker-compose -f ./assets/docker/compose/test.yml run ironsource-test",
    "config-then-test": "gulp config && npm test",
    "config-then-run": "gulp config && npm start"
  },
  "author": "Nati Nazarov",
  "license": "ISC",
  "browserify": {
    "transform": [
      "browserify-css",
      "lessify"
    ]
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-react-jsx",
      "syntax-async-functions",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "bowser": "^1.5.0",
    "bunyan": "^1.8.4",
    "bunyan-prettystream": "^0.1.3",
    "bunyan-request": "^1.2.0",
    "cors": "^2.8.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-useragent": "^1.0.4",
    "express-validator": "^2.21.0",
    "font-awesome": "^4.7.0",
    "form-urlencoded": "^1.5.0",
    "iso-countries": "0.0.5",
    "leveldown": "^1.5.0",
    "levelup": "^1.3.3",
    "live-server": "^1.1.0",
    "moment": "^2.16.0",
    "nconf": "^0.8.4",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "react-spinkit": "^1.1.11",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "retry": "^0.10.0",
    "selectn": "^1.1.1",
    "through2": "latest",
    "watchify": "^3.7.0",
    "whatwg-fetch": "^2.0.1",
    "yargs": "^6.4.0",
    "glob": "latest"
  },
  "devDependencies": {
    "apidoc": "git://github.com/Natinux/apidoc",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-css": "^0.9.2",
    "chai": "^3.5.0",
    "cssify": "^1.0.3",
    "del": "^2.2.2",
    "fast-async": "^6.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-extend": "^0.2.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-util": "latest",
    "gulp-watch": "^4.3.10",
    "less": "^2.7.1",
    "lessify": "^1.0.1",
    "mkdirp": "latest",
    "mocha": "^3.2.0",
    "redux-logger": "^2.7.4",
    "run-sequence": "^1.2.2",
    "vinyl-source-stream2": "^0.1.1"
  }
}
